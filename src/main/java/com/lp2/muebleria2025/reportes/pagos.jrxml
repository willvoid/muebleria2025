	<parameter name="pago_id" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT p.*, c.nombre, t.*  
FROM pagos p
INNER JOIN cliente c ON c.id = p.idcliente
INNER JOIN transaccion t ON t.id_pago = p.idpagos
